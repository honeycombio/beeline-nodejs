{
  "name": "honeycomb-nodejs-magic",
  "version": "1.0.0-beta5",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "format": "prettier --write \"lib/**/*.js\"",
    "check-format": "prettier \"lib/**/*.js\"",
    "lint": "eslint \"lib/**/*.js\"",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx}": ["prettier --parser flow --write", "eslint", "git add"],
    "*.json": ["prettier --parser json --write", "git add"],
    "*.{graphql,gql}": ["prettier --parser graphql --write", "git add"],
    "*.{md,markdown}": ["prettier --parser markdown --write", "git add"],
    "*.{css,scss}": ["prettier --parser css --write", "git add"]
  },
  "author": "support@honeycomb.io",
  "license": "Apache-2.0",
  "devDependencies": {
    "supertest": "^3.0.0",
    "express": "^4.16.2",
    "eslint": "^4.17.0",
    "husky": "^0.14.3",
    "jest": "^22.2.2",
    "lint-staged": "^6.1.0",
    "prettier": "^1.10.2"
  },
  "dependencies": {
    "debug": "^3.1.0",
    "libhoney": "^1.1.0",
    "on-headers": "^1.0.1",
    "shimmer": "^1.2.0",
    "uppercamelcase": "^3.0.0",
    "uuid": "^3.2.1"
  },
  "jest": {
    "verbose": true,
    "timers": "real"
  }
}
